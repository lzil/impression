extends layout

block content
  script(type='text/javascript' src='/javascripts/results.js')
  div(style='padding:15px')
    a(href='/' class='backbutton')
      i(class="fa fa-chevron-circle-left")
      &nbsp;back to search
  div(class='results-wrapper container-fluid')
    div(class='col-md-12 result-title')
      h1 Here's your playlist:
    div(class='row')
      div(class='col-md-6' id='results-table')
        span(class='invis' style='display:none')
        div(id="pop")
      div(class='song-details col-md-6')



  div(id="icon-wrapper")
    p#tags(style="display:none;")= cResults
